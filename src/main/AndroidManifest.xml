<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sidorovoleg.ipstore"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sidorovoleg.ipstore.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sidorovoleg.ipstore.SecondAct"
            android:label="@string/title_activity_second" >
        </activity>
        <activity
            android:name="com.sidorovoleg.ipstore.PrefAct"
            android:label="PrefActivity" />
        <activity
            android:name=".DetailedAct"
            android:label="@string/title_activity_detailed_afc" >
        </activity>
    </application>

</manifest>
